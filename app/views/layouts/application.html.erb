<!DOCTYPE html>
<html>
<head>
  <title>Flickr</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= stylesheet_link_tag "styles.css" %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>


  <nav class="navbar navbar-default navbar-dark">
  <div class="container-fluid">
    <div class="navbar-header">
      <%= link_to "Flickr", root_url, class: "navbar-brand" %>
    </div>
    <ul class="nav navbar-nav">
      <li class="active"><%= render 'devise/menu/login_items' %></li>
      <li><%= link_to "Post Image", new_image_path %></li>
      <li><%= render 'devise/menu/registration_items' %></li>
      <!-- <li><a href="#">Page 3</a></li> -->
    </ul>
  </div>
</nav>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
  <!-- Page Content -->
  <div class="container">
    <div class="row">
      <div class="col-lg-12 text-center">

        <%= yield %>
      </div>
    </div>
  </div>



</body>
</html>
